    },
    // Options for the "large" panels - the widest panels.
    // ????? ???? "large" - ??????? ??? ? ????.
    "large": {
      // false - disable (?????????)
      "enabled": true,
      // Minimum width of the player's name column, 0-250. Default is 170.
      // ??????????? ?????? ???? ????? ??????, 0-250. ?? ?????????: 170.
