    },
    // Options for the "medium" panels - the first of the medium panels.
	// ????? ???? "medium" - ?????? ??????? ??? ? ????.
    "medium": {
      // false - disable (?????????)
      "enabled": true,
      // Minimum width of the player's name column, 0-250. Default is 46.
      // ??????????? ?????? ???? ????? ??????, 0-250. ?? ?????????: 46.
      "width": 60,
      // true - disable platoon icons
      // true - ?????? ??????????? ?????? ??????
      "removeSquadIcon": false, 
      // Display format for the left panel (macros allowed, see macros.txt).
      // ?????? ??????????? ??? ????? ?????? (??????????? ????????????????, ??. macros.txt).
      "formatLeft": "<font color='#DDDDDD'><font face='mono' size='12' color='{{c:kb}}'>{{kb%2d~k|--k}}</font> <font face='Webdings' size='9' color='{{c:xte}}'>n</font><font size='5'> </font><font face='Webdings' size='11' color='{{c:wn8}}'>n</font><font size='5'> </font><font face='Webdings' size='9' color='{{c:t-rating}}'>n</font></font> <img src='xvm://res/icons/xvm/xvm-user-{{xvm-user|none}}.png'> ",
      // Display format for the right panel (macros allowed, see macros.txt).
      // ?????? ??????????? ??? ?????? ?????? (??????????? ????????????????, ??. macros.txt).
