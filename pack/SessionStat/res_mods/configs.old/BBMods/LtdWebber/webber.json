{
	"SkinDescription": {
		"Author": "Ltd Webber",
		"Name": "Einfache Sitzungsstatistik",
		"Version": "1.3",
		"Description": "Sitzungsstatistik mit Panzer- und Karten체bersicht"
	},
	
	"ScrollStep": 50,
	"PopUpResultLife": 12000.0,
	"PopUpResultHiding": 1000.0,
	"OriginalResults": false,
	"InBattleResult": true,

	"StatisticSessions": {
		"Current": {
			"Reset": {"ResetAfter": 1, "ResetTime": 6},
			"Battles": [1]
		}
	},

	"Pages": {
		"First": "Current",
		"NoBattle": {
			"template": [
				"<p align='center'><font color='#E0E0E0' size='16'><b>{{UserName}}\nKeine Daten. Mach ein Gefecht</b></font></p>",
				"\n\n<font size='12' color='#545454'>",
				"Mod: <a href='event:{{ModURL}}'><font color='#545454'>{{ModVersion}}</font></a>\n",
				"Skin: '{{SkinName}}' ver:{{SkinVersion}}\n",
				"Skin Author: {{SkinAuthor}}\n\n",
				"<p align='justify'>{{SkinDescription}}</p></font>"
			],
			"icon": "",
			"bgIcon": "img://../res_mods/configs/BBMods/LtdWebber/images/bg/background_default.png"
		},
		"Current": {
			"template": [
				"<p align='center'><font size='16' color='#0773EF'><b><u>Sitzungsstatistik</u></b></font>\n<font size='14' color='#0773EF'>Zufallsgefechte</font></p>\n",
				"<textformat leading='0' tabstops='85,110,164'><font size='13' color='#C0C0C0'>",
				"Gefechte/Siege:\t<img src='img://gui/maps/icons/library/BattleResultIcon-1.png'width='13' height='13' vspace='-2'>\t<b>{{Current_BattleCounter}}/{{Current_WinCounter}} <font color='{{c:Current_NumWinRate}}'>{{Current_NumWinRate:0f}}</font>\t( <font color='{{c:Current_WinCounter:r}}'>{{Current_WinCounter:r}}%</font> )</b>\n",
				"Erfahrung(x1):\t<img src='img://gui/maps/icons/library/XpIcon.png'width='13' height='13'>\t<b>{{Current_OriginalXP}}\t( <font color='{{c:Current_OriginalXPAvg}}'>{{Current_OriginalXPAvg}}</font> )</b>\n",
				"Erfahrung:\t<img src='img://gui/maps/icons/library/multyXp.png'width='13' height='13'>\t<b>{{Current_XP}}\t( <font color='{{c:Current_XPAvg}}'>{{Current_XPAvg}}</font> )</b>\n",
				"Freie XP:\t<img src='img://gui/maps/icons/library/FreeXpIcon.png'width='13' height='13'>\t<b>{{Current_FreeXP}}\t( <font color='{{c:Current_FreeXPAvg}}'>{{Current_FreeXPAvg}}</font> )</b>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>",
				"Schaden:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/damage.png' width='13' height='13'>\t<b>{{Current_DamageDealt}}\t( <font color='{{c:Current_DamageDealtAvg}}'>{{Current_DamageDealtAvg}}</font> )</b>\n",
				"Sch체sse/Treffer:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/shotDispersionAngle.png' width='13' height='13'>\t<b>{{Current_Shots}}/{{Current_Hits}}\t( <font color='{{c:Current_HitsRatio}}'>{{Current_HitsRatio}}%</font> )</b>\n",
				"Durchschl채ge:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/piercingPower.png' width='13' height='13'>\t<b>{{Current_Piercings}}/{{Current_Hits}}\t( <font color='{{c:Current_DamageHitsRatio}}'>{{Current_DamageHitsRatio}}%</font> )</b>\n",
				"Absch체sse:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/Kills.png' width='13' height='13'>\t<b>{{Current_Kills}}\t( <font color='{{c:Current_KillsAvg}}'>{{Current_KillsAvg:2f}}</font> )</b>\n",
				"Gestorben:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/alertIcon.png' width='13' height='13'>\t<b>{{Current_DeathCount}}</font>\t( <font color='{{c:Current_DeathCount:r}}'>{{Current_DeathCount:r}}%</font> )</b>\n",
				"Gesichtet:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/circularVisionRadius.png' width='13' height='13'>\t<b>{{Current_Spotted}}\t( <font color='{{c:Current_SpottedAvg}}'>{{Current_SpottedAvg:2f}}</font> )</b>\n",
				"Schaden assist.:\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/assist.png' width='13' height='13'>\t<b>{{Current_Assisted}}\t( <font color='{{c:Current_AssistedAvg}}'>{{Current_AssistedAvg}}</font> )</b>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>",
				"<textformat leading='0' tabstops='85,110,164'>",
				"Tier(Gefecht):\t<img src='img://../res_mods/configs/BBMods/LtdWebber/images/level.png' width='13' height='13'>\t<b>{{Current_TankLevelAvg:1f}}\t(<font color='{{c:Current_BattleLevelAvg}}'>{{Current_BattleLevelAvg:1f}}</font> )</b>\n",
				"Rundenzeit:\t<img src='img://gui/maps/icons/library/timerIcon.png'width='13' height='13'>\t<b>{{Current_BattleTimer:t}}\t( <font color='{{c:Current_BattleTimerAvg}}'>{{Current_BattleTimerAvg:t}}</font> )</b>\n",
				"SessionZeit:\t<img src='img://gui/maps/icons/library/timerIcon.png'width='13' height='13'>\t<b>{{Current_SessionOnline}}</b>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>",
				"Credits / Netto:\t<img src='img://gui/maps/icons/library/CreditsIcon-2.png'width='18' height='18' vspace='-4'>\t<b>{{Current_Credits}}\t<font color='{{c:Current_CreditDebit}}'>{{Current_CreditDebit}}</font></b>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>",
				"<textformat leading='1' tabstops='120'><font size='15' color='#C0C0C0'>\n",
				"\nWN8: <font color='{{c:Current_WN8}}'><b>{{Current_WN8}}</b></font> (<font color='{{c:Current_XWN8}}'><b>{{Current_XWN8}}</b></font>)\tWGR: <font color='{{c:WGR}}'><b>{{WGR}}</b></font>\n",
				"<p align='center'>\n{{um:btn_overall_}}{{um:btn_tanks}}{{um:btn_maps}}</p>"
			],
			"icon": "img://../res_mods/configs/BBMods/LtdWebber/images/stat.png",
			"bgIcon": "img://../res_mods/configs/BBMods/LtdWebber/images/bg/{{c:Current_WN8}}.png"
		},
		"Maps": {
			"template": [
				"<p align='center'><font size='16' color='#0773EF'><b>Kartenstatistik</b></font></p><textformat leading='0' tabstops='30,137,160,200'>\n",
				"\tKarte\t<img src='img://gui/maps/icons/statistic/battles24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/wins24.png' width='24' height='24' vspace='-7'>\tTier\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1' vspace='0'>\n",
				"<textformat tabstops='145,165,197'>{{Current_MapsStat}}",
				"<p align='center'>\n{{um:btn_overall}} {{um:btn_tanks}}{{um:btn_maps_}}</p>"
			],
			"bgIcon": "img://../res_mods/configs/BBMods/LtdWebber/images/bg/{{c:Current_WN8}}.png"
		},
		"Tanks": {
			"template": [
				"<p align='center'><font size='16' color='#0773EF'><b>Panzerstatistik</b></font></p><textformat leading='0' tabstops='25,88,113,153,192'>\n",
				"\t<img src='img://gui/maps/icons/buttons/Tank-ico.png' width='28' height='12' vspace='1'>",
				"\t<img src='img://gui/maps/icons/statistic/battles24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/wins24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/avgDamage24.png' width='24' height='24' vspace='-7'>\tWN8\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1' vspace='0'>\n",
				"<textformat tabstops='97,115,152,190'>{{Current_TanksStat}}",
				"<p align='center'>\n{{um:btn_overall}} {{um:btn_tanks_}}{{um:btn_maps}}</p>"
			],
			"bgIcon": "img://../res_mods/configs/BBMods/LtdWebber/images/bg/{{c:Current_WN8}}.png"
		}
	},

	"Current_MapsStat": {
		"SortKey": "BattleCounter#des",
		"template": ["<font color='#A1A1A1'>{{Name}}</font>\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>\t<font color='{{c:BattleLevelAvg}}'>{{BattleLevelAvg:1f}}</font>\n"]
	},

	"Current_TanksStat": {
		"SortKey": "BattleCounter#des",
		"template": [
			"<img src='img://gui/maps/icons/library/proficiency/class_icons_{{MarkOfMastery}}.png' width='20' height='20' vspace='-6'/><font color='#A1A1A1'>{{TankNameShort}}</font>",
			"\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>",
			"\t<font color='{{c:DamageDealtAvg}}'>{{DamageDealtAvg}}</font>\t<font color='{{c:XWN8}}'>{{WN8}}</font>\n"
		]
	},

	"Current_BattleTimer": "%H:%M:%S",
	"Current_SessionOnline": "%H h. %M m. %S sek.",

	"ResultName": {"Win": "Sieg", "Defeat": "Niederlage", "Draw": "Unentschieden"},

	"InBattleResultPattern": ["<img src='img://../res_mods/configs/BBMods/LtdWebber/images/{{BattleResultName}}.png' width='150' height='50' align='baseline' vspace='0'>\n{{um:ChatResults}}"],

	"ResultPattern": {
		"template": ["<font color='{{c:BattleResult}}' size='18'><b>{{BattleResultName}}</b></font>\n\n{{um:Results}}"]
	},


	"Achievements": {
		"Battle": {
			"Header": "\n<p align='center'><b>Errungenschaften:</b></p><font color='#D3D3D3'>",
			"Body": "{{AchName}}",
			"Separator": ", ",
			"Footer": "</font>"
		}
	},

	"UserMacros": {
		"um:Results": [
			"<p>Map: <font color='#D3D3D3'>{{BattleMapName}} - {{BattleTankName}}</font></p>",
			"<p>Credits: <font color='#D3D3D3'>{{BattleCredits}}</font></p>",
			"<p>NettoCredits: <font color='{{c:BattleCreditDebit}}'>{{BattleCreditDebit}}</font></p>",
			"<p>Erfahrung: <font color='#D3D3D3'>{{BattleXP}}</font></p>",
			"<p>Crew XP: <font color='#D3D3D3'>{{BattleTmenXP}}</font></p>",
			"<p>Schaden: <font color='#D3D3D3'>{{BattleDamageDealt}} / {{BattleBlockedByArmor}} / {{BattleDamageRating}}%</font></p>",
			"<p>WN8:\t<font color='{{c:BattleWN8}}'>{{BattleWN8}}</font><p>",
			"{{BattleAchievements}}{{um:ButtonResult}}"
		],
		"um:ChatResults": [
			"<textformat tabstops='[100]'><img src='img://gui/maps/icons/library/TournamentBattleResultIcon-1.png' width='15' height='15' vspace='-3'> <font color='#CCCCCC'>Map:</font>\t<font color='#ffe769'>{{BattleMapName}}</font>\n",
			"<img src='img://gui/maps/icons/library/ClanBattleResultIcon-1.png' width='15' height='15' vspace='-2'> <font color='#CCCCCC'>Panzer:</font>\t<font color='#ffe769'>{{BattleTankName}}</font>\n",
			"<img src='img://gui/maps/icons/library/CreditsIcon-1.png' width='15' height='15' vspace='-2'> <font color='#CCCCCC'>Credits:</font>\t<font color='#ffe769'>{{BattleCredits}}</font>\n",
			"<img src='img://gui/maps/icons/library/EliteXpIcon-2.png' width='15' height='15' vspace='-2'> <font color='#CCCCCC'>XP:</font>\t<font color='#ffe769'>{{BattleXP}}</font></textformat>"
		],
		"um:ButtonCurrent": ["<a href='event:Page:Current'><b>Stats</b></a>"],
		"um:ButtonReset": ["<a href='event:Reset'><img src='img://../configs/BBMods/LtdWebber/images/buttons/reset.png'></a>"],
		"um:ButtonMaps": ["<a href='event:Page:Maps'><img src='img://gui/maps/icons/orders/small/militaryExercises.png' width='26' height='26' vspace='-15'><b>Maps</b></a>"],
		"um:btn_overall" : "<a href='event:Page:Current'><img src='img://../configs/BBMods/LtdWebber/images/buttons/overall.png' vspace='1'></a>",
		"um:btn_overall_" : "<img src='img://../configs/BBMods/LtdWebber/images/buttons/overall_.png' vspace='1'>",
		"um:btn_overall__" : "<img src='img://../configs/BBMods/LtdWebber/images/buttons/overall__.png' vspace='1'>",
		"um:btn_maps" : "<a href='event:Page:Maps'><img src='img://../configs/BBMods/LtdWebber/images/buttons/maps.png' vspace='1'></a>",
		"um:btn_maps_" : "<img src='img://../configs/BBMods/LtdWebber/images/buttons/maps_.png' vspace='1'>",
		"um:btn_tanks" : "<a href='event:Page:Tanks'><img src='img://../configs/BBMods/LtdWebber/images/buttons/tanks.png' vspace='1'></a>",
		"um:btn_tanks_" : "<img src='img://../configs/BBMods/LtdWebber/images/buttons/tanks_.png' vspace='1'>",
		"um:ButtonTanks": ["<a href='event:Page:Tanks'><img src='img://gui/maps/icons/orders/small/tacticalTraining.png' width='26' height='26' vspace='-15'><b>Panzer</b></a>"],
		"um:ButtonResult": ["\n<a href='event:showBattleResults'><img src='img://../configs/BBMods/LtdWebber/images/buttons/btn_details.png'></a>"]
	},

	"UserPattern": {
		"up:BattleMastery": ["<img src='img://gui/maps/icons/library/proficiency/class_icons_{{BattleMastery}}.png' width='28' height='28' vspace='-10'/>", ""],
		"up:BattleDamageRating": ["<img src='img://gui/maps/icons/marksOnGun/95x85/{{BattleTankNation}}_1_mark.png' width='15' height='15' vspace='-8'><font color='{{c:BattleDamageRating}}'>{{BattleDamageRating}}%</font>", ""]
	}
}
