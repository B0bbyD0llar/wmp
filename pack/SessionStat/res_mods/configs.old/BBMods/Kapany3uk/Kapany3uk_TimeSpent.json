{
    // Файл цветовых настроек.
	${"Kapany3uk/_Colors.json"},

    // Файл перевода.
    ${"Kapany3uk/_Translation.json"},

    // Описание скина.
    "SkinDescription": {
        // Автор конфигурации.
        "Author": "Kapany3uk",
        // Название конфигурации.
        "Name": "txt:Mod_name",
        // Версия конфигурации.
        "Version": "#3.3",
        // Описание конфигурации.
        "Description": ""
    },

    // Настройка скорости пролистывания сообщений в системном канале. (Стандартное значение: 10) 
    "ScrollStep": 10,

    // Время отображения PopUp сообщения c результатами боя в ангаре. Стандартное значение 6000.0
    "PopUpResultLife": 12000.0,

    // Время "растворения" PopUp сообщения c результатами боя в ангаре.  Стандартное значение 2000.0
    "PopUpResultHiding": 2000.0,

    // Переключатель вывода результатов боя в ангаре. false - результаты мода, true - оригинальные результаты.
    "OriginalResults": false,

    // Переключатель вывода результатов предыдущего незаконченного боя в боевой чат. true - выводить, false - не выводить.
    "InBattleResult": true,

    // Настройка сессий результатов статистики.
    "StatisticSessions": {
        // Статистика по всем значимым боям.
        "Overall": {
            // Настройка автоматического сброса статистики.
            // "ResetAfter" - Автоматически обнулять статистику по истечении указанного кол-ва дней. 
            // Если текущая дата превышает SessionStart + ResetAfter, то вне зависимости от ResetTime, статистика будет сброшена.
            // При ResetAfter = 0 статистика не будет сбрасываться автоматически.
            // "ResetTime" - Час после которого обнуляется статистика. Статистика обнулится если текущий час >= ResetTime
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            // Настройка типов боев учитываемых данной сессией.
            "BattleTypes": [  0, 1, 3, 4, 5, 7, 10, 11, 12, 13, 14, 15, 20, 21 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '1.Случайный' '3.Ротный'
        "Current": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  1, 3 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '105.Случайный бой взводом'
        "Squad": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  105 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '106.Случайный бой соло'
        "Alone": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  106 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '0.Специальный' '5.CLAN' '10.Укрепрайон, вылазка' '11.Битва за укрепрайон' '13.Глобальная карта' '15.TOURNAMENT_CLAN'
        "Clanwar": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  0, 5, 10, 11, 13, 15, 20, 21 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '4.Турнир' '7.Командный' '12.Ладдер' '14.TOURNAMENT_REGULAR'
        "Teams": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  4, 7, 12, 14 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // Бои, статистика по которым не собирается: '2.Тренировочный' '6.Обучающий' '8.Исторический' 
        // '9.Танковый футбол' '16.Учебный' '17.Учебный с ботами' '18.Бой до последнего' '19.Бой до последнего - командный'
        "Unused": {
            "Reset": {"ResetAfter": 0, "ResetTime": 0},
            "BattleTypes": [  2, 6, 8, 9, 16, 17, 18, 19 ],
			"SectionsList": ["Statistic"]
        },
        
        // -------------- СУТКИ -----------------------
        // Статистика по всем значимым боям.
        "DOverall": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  0, 1, 3, 4, 5, 7, 10, 11, 12, 13, 14, 15, 20, 21 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '1.Случайный' '3.Ротный'
        "DCurrent": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  1, 3 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '105.Случайный бой взводом'
        "DSquad": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  105 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '106.Случайный бой соло'
        "DAlone": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  106 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '0.Специальный' '5.CLAN' '10.Укрепрайон, вылазка' '11.Битва за укрепрайон' '13.Глобальная карта' '15.TOURNAMENT_CLAN'
        "DClanwar": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  0, 5, 10, 11, 13, 15, 20, 21 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        },
        // '4.Турнир' '7.Командный' '12.Ладдер' '14.TOURNAMENT_REGULAR'
        "DTeams": {
            "Reset": { "ResetAfter": 1, "ResetTime": 0, "LocalTime": false },
            "BattleTypes": [  4, 7, 12, 14 ],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
        }
    },

	// Строка, выводимая макросом {{BattleResultName}} в зависимости от результата боя.
	"ResultName": { "Win": "txt:Win!", "Defeat": "txt:Defeat", "Draw": "txt:Draw" },

    // Шаблон сообщений результатов предыдущего незаконченного боя в боевой чат.
    "InBattleResultPattern": [
        "<textformat tabstops='[25,85]' leading='0'>",
        "<img src='img://gui/maps/icons/library/BattleResultIcon-1.png' width='16' height='16' vspace='-2'>",
        "\t<font face='Segoe Print' size='18' color='{{c:BattleResult}}'>{{BattleResultName}}</font>\n",
        "<font face='PartnerUltraCondensed' size='17' color='#A09C94'>",
        "\ttxt:1Battle:\t<font color='#D7D7A7'>{{BattleMapName}}</font>\n",
        "\ttxt:Vehicle:\t<font color='#D7D7A7'>{{BattleTankName}}</font>\n",
        "\ttxt:Received:\t<img src='img://../configs/BBMods/Kapany3uk/img/{{BattleDailyXPFactor}}Xp.png' vspace='-3'>",
        "<font color='#FFBBFF'>{{BattleXP}}</font>",
        "    <img src='img://../configs/BBMods/Kapany3uk/img/credits2.png' vspace='-3'>",
        "<font color='#FFE87F'>{{BattleCredits}}</font></font>",
        "</textformat>"
    ],

    // Шаблон для вывода сообщений результатов боя в центр уведомлений.
    "ResultPattern": {
        "template": [
            "<font face='PartnerUltraCondensed' size='15'>",
            "<textformat leading='-15'>",
            "<font face='Segoe Print' size='18' color='{{c:BattleResult}}'>{{BattleResultName}}</font>\n",
            "<img src='img://../configs/BBMods/Kapany3uk/img/empty.png' height='8'>\n",
            "<p align='right'>",
            "<img src='img://gui/maps/icons/library/PersonalAchievementsIcon-1.png' vspace='-3'>",
            "<font size='1' alpha='#0'>| WN8: </font>",
            "<font color='{{c:BattleWN8}}'>{{BattleWN8}}</font>",
            "  <img src='img://../configs/BBMods/Kapany3uk/img/medal.png' vspace='-4'>",
            "<font size='1' alpha='#0'> | txt:eff: </font>",
            "<font color='{{c:BattleEFF}}'>{{BattleEFF}}<font size='1' alpha='#0'> |</font></font></p></textformat>\n",
            "<img src='img://../configs/BBMods/Kapany3uk/img/empty.png' height='20'>\n",
            "<textformat tabstops='[20,88,160,180]' leading='1'>",
            " \ttxt:1Battle:\t<font color='#D7D7A7'>{{BattleMapName}}<font size='1' alpha='#0'> |</font></font>\n",
            " \ttxt:Vehicle:\t<font color='#D7D7A7'>{{BattleTankNameShort}}<font size='1' alpha='#0'> |</font></font>\n",
            "<img src='img://../configs/BBMods/Kapany3uk/img/{{BattleDailyXPFactor}}Xp.png' vspace='-3'>",
            "\ttxt:Experience:\t<font color='#FFBBFF'>{{BattleXP}}<font size='1' alpha='#0'> (</font></font>",
            "\t\t<font color='{{c:BattleOriginalXP}}'>{{BattleOriginalXP}}<font size='1' alpha='#0'> )</font></font>\n",
            " \ttxt:Credits:\t<font color='{{c:BattleCredits}}'>{{BattleCredits}}<font size='1' alpha='#0'> (</font></font>",
            "\t\t<font color='{{c:BattleCreditDebit}}'>{{BattleCreditDebit}}<font size='1' alpha='#0'> )</font></font>\n",
            " \ttxt:Damage:\t<font color='{{c:BattleRatioDamageWN8}}'>{{BattleDamageDealt}}</font>{{up:BattleTeamDamage}}",
            "\t\t<font size='1' alpha='#0'>| txt:Frags: </font><font color='{{c:BattleKills}}'>{{BattleKills}}</font>",
            "{{up:BattleXPPenalty}}{{up:BattleContributionIn}}",
            "{{BattleAchievements}}",
            "</textformat></font>",
            "\n<img src='img://../configs/BBMods/Kapany3uk/img/delimiter.png'>\n",
            "<a href='event:showBattleResults'>txt:btn_detail</a>"
        ],
        "icon": "",
        "bgIcon": "img://../configs/BBMods/Kapany3uk/img/bg/{{BattleResult}}_small.png"
    },

    // Форматирование списка боевых достижений.
    "Achievements": {
        "Battle": {
            // Заголовок списка достижений
            "Header": "<textformat leading='-5'>\n<img src='img://../configs/BBMods/Kapany3uk/img/Planka_{{BattleResult}}.png' width='220' height='18' vspace='9'><font size='17' color='#D7D7A7'><font size='1' alpha='#0'>| txt:AchHead </font>",
            // Шаблон одной строки списка достижений. 
            "Body": "<img src='{{AchIcon}}' width='34' height='36' align='baseline' vspace='-25'> {{AchName}}",
            // Разделитель списка достижений
            "Separator": "\n<font size='1' alpha='#0'>, </font>",
            // Конец списка достижений
            "Footer": "</font></textformat>"
        }
    },

    // Страницы с выводимой информацией.
    "Pages": {
        // Имя страницы которая будет отображаться первой.
        "First": "Overall",
        // Шаблон сообщения при отсутствии боев.
        "NoBattle": {
            // шаблон выводимой информации
            "template": [
                "<font face='PartnerUltraCondensed' size='15'><p align='center'>",
                "<a href='event:{{ModURL}}'><font face='Segoe Print' color='#FFE87F'><b>{{SkinName}}</b></font></a>\n\n",
                "<a href='event:Page:Summary'><font color='#8B8C7E'>txt:session_stat</font></a>\n\n",
                "<font size='13'>txt:blank_page<font size='7'>\n\n</font>[txt:data_empty]\n\n",
                "<font color='#1b1b0d'>txt:skin {{SkinVersion}} txt:edited_by '{{SkinAuthor}}'</font></font></p>",
                "\n{{um:footer}}{{um:delimiter}}",
                "<a href='event{{up:Overall_isReset}}'>{{um:btn_overall}}</a>",
                "<a href='event{{up:Current_isReset}}'>{{um:btn_current}}</a>",
                "<a href='event{{up:Clanwar_isReset}}'>{{um:btn_clanwar}}</a>",
                "<a href='event{{up:Teams_isReset}}'>{{um:btn_teams}}</a>\t{{um:btns}}</textformat>"
            ],
            // Для использования стандартной иконки - за комментируйте параметр, для отключения иконки - задайте значение ""
            // Путь к боковой иконке.
            "icon": "",
            // Путь к фоновой картике 
            "bgIcon": "img://../configs/BBMods/Kapany3uk/img/bg/nobattle.png"
        },

        // Справочная страница.
        "Helpme": {
            "template": [
                "<textformat tabstops='[53]' leading='-2'><font face='PartnerUltraCondensed' size='15'>",
                "<font size='17' color='#D7D7A7'>txt:Readme &#34;{{SkinAuthor}}&#34;</font><font size='16'>\n\n</font>",
                "<font color='#FFE87F'>\u00ABtxt:Overall\u00BB</font>\ttxt:summary\n",
                "txt:except\ntxt:historical<font size='6'>\n\n</font>",
                "<font color='#FFE87F'>\u00ABtxt:Random\u00BB</font>\ttxt:company<font size='6'>\n\n</font>",
                "<font color='#FFE87F'>\u00ABtxt:Clan\u00BB</font>\ttxt:Global_Map\n",
                "txt:skirmishes<font size='6'>\n\n</font>",
                "<font color='#FFE87F'>\u00ABtxt:Team\u00BB</font>\ttxt:ranked\n",
                "txt:championships<font size='6'>\n\n</font>",
                "<font color='#FFE87F'>\u00ABtxt:1Battle\u00BB</font>\ttxt:extended",
                "\ntxt:to_view txt:press <a href='event:Page:LastBattle'>{{um:btn_goto}}</a> txt:page",
                "<font size='6'>\n\n</font>",
                "txt:Button <a href='event:Page:Finally'>{{um:btn_reset_13}}</a> txt:to_reset\n",
                "<font color='#FF7A00'>txt:attention txt:data_deletion</font><font size='6'>\n\n</font>",
                "      - txt:avg_battles ",
                "<img src='img://../configs/BBMods/Kapany3uk/img/tank_lvl.png' vspace='-3'>- txt:avg_tanks\n",
                "      - txt:avg_duration",
                "<img src='img://../configs/BBMods/Kapany3uk/img/lifeTime.png' vspace='-3'>- txt:avg_life_time\n",
                "      - txt:spent_gold<font size='7'>\n\n</font>",
                "<font color='#D7D7A7'>txt:short_info</font>",
                "{{um:footer}}",
                "</textformat><textformat tabstops='[18,137]' leading='-15'>",
                "<font face='PartnerUltraCondensed' size='15'>",
                "\n<{{um:height}}='26'><font face='PartnerUltraCondensed' size='18' color='#FFE87F'>txt:headerUnused\n",
                    "\n<{{um:height}}='16'>\n<p align='right'><textformat rightMargin='22'>",
                    "<font size='13'>{{Unused_SessionStart}}</font></textformat>",
                    "\n<{{um:height}}='15'>\n<a href='event:Reset:Unused'>{{um:btn_reset}}</a></p>\n",
                "<{{um:height}}='24'>\n   \ttxt:Battles\ttxt:Income",
                    "\n<{{um:height}}='15'>\n<p align='right'><textformat rightMargin='{{um:Margin}}'>",
                    "<font color='{{c:Unused_BattleCounter}}'>{{Unused_BattleCounter}}</font>",
                    "/<font color='#00AF00'>{{Unused_WinCounter}}</font></textformat>",
                    "\n<{{um:height}}='15'>\n<font color='{{c:Unused_Credits}}'>{{Unused_Credits}}</font></p>\n",
                "<{{um:height}}='32'>\n   \ttxt:Costs\ttxt:Expenses",
                    "\n<{{um:height}}='15'>\n<p align='right'><textformat rightMargin='{{um:Margin}}'>",
                    "<font color='{{c:Unused_Expenses}}'>{{Unused_ExpensesG}}</font></textformat>",
                    "\n<{{um:height}}='15'>\n<font color='{{c:Unused_Expenses}}'>{{Unused_Expenses}}</font></p>\n",
                "<{{um:height}}='32'>\n   \t<font color='{{c:Unused_TotalXP}}'>txt:Experience</font>",
                    "\t<font color='{{c:Unused_Credits}}'>txt:Profit</font>",
                    "\n<{{um:height}}='15'>\n<p align='right'><textformat rightMargin='{{um:Margin}}'>",
                    "<font color='{{c:Unused_XPAvg}}'>{{Unused_XP}}</font></textformat>",
                    "\n<{{um:height}}='16'>\n<font color='{{c:Unused_CreditDebitAvg}}'>{{Unused_CreditDebit}}</font></p>\n",
                "<{{um:height}}='43'>\n{{um:footer}}",
                "<a href='event{{up:Overall_isReset}}'>{{um:btn_overall}}</a>",
                "<a href='event{{up:Current_isReset}}'>{{um:btn_current}}</a>",
                "<a href='event{{up:Clanwar_isReset}}'>{{um:btn_clanwar}}</a>",
                "<a href='event{{up:Teams_isReset}}'>{{um:btn_teams}}</a>\t",
                "<a href='event:Page:LastBattle'>{{um:btn_battle}}</a>",
                "<a href='event:Page:Summary'>{{um:btn_help_}}</a></textformat>"
            ],
            "icon": "",
            "bgIcon": "img://../configs/BBMods/Kapany3uk/img/bg/help.png"
        }, 

        // Шаблон окна для результатов последнего боя.
        "LastBattle": {
            "template": [
                "{{um:headerSP}}<font color='#FFE87F'>txt:headerBattle",
                "<font color='{{c:BattleResult}}'>{{BattleResultName}}</font></font>{{um:header}}",

                "\n<{{um:height}}='6'>\n\t<font color='#D7D7A7'>{{BattleMapName}}</font>\tWN8",
                "\n\t<font color='#D7D7A7'>{{BattleTankNameShort}}</font>\ttxt:wn8_ktts",
                "\n\ttxt:life_time\ttxt:eff\n<{{um:height}}='5'>\n\ttxt:Income\ttxt:Repair",
                "\n\ttxt:Boosters\ttxt:Shells\n\ttxt:Missions\ttxt:Consumables\n\ttxt:Compens\ttxt:Penalty",
                "\n\t<font color='{{c:BattleCredits}}'>txt:Profit</font>\ttxt:Costs",
                "\n<{{um:height}}='5'>\n\t<font color='{{c:BattleTotalXP}}'>txt:Total_exp</font>",
                    "\t<font color='{{c:BattleTmenXP}}'>{{BattleTmenXP}}</font></textformat>",
                "\n\ttxt:Boosters\t<font color='{{c:BattleBoosterTMenXP}}'>{{BattleBoosterTMenXP}}</font>",
                "\n<{{um:height}}='13'>\ttxt:Missions\t<font color='{{c:BattleEventTMenXP}}'>{{BattleEventTMenXP}}</font>",
                "\n\t<font color='{{c:BattleTotalXP}}'>txt:Original</font>\ttxt:Fine",
                "\n<{{um:height}}='5'>\n\ttxt:Destroyed\ttxt:Capture\n\ttxt:Damage\ttxt:Defense",
                "\n\ttxt:Dmg_ratio\ttxt:Spotted\n\ttxt:Assistance\ttxt:Hit_ratio",

                "\n<textformat leading='-145'>\n<{{um:height}}='9'>\n</textformat><p align='right'>",
                "<textformat rightMargin='{{um:Margin}}'>",
                "\n<{{um:height}}='7'>\n<font color='{{c:BattleLifeTime}}'>{{BattleLifeTime:t}}</font>",
                "\n<{{um:height}}='6'>\n<font color='{{c:BattleCredits}}'>{{BattleCredits}}</font>",
                "\n<font color='{{c:BattleCredits}}'>{{BattleBoosterCredits}}</font>",
                "\n<font color='{{c:BattleCredits}}'>{{BattleEventCredits}}</font>",
                "\n<font color='{{c:BattleCredits}}'>{{BattleContributionIn}}</font>",
                "\n<font color='{{c:BattleCreditDebit}}'>{{BattleCreditDebit}}</font>",
                "\n<{{um:height}}='5'>\n<font color='{{c:BattleXP}}'>{{BattleXP}}</font>",
                "\n<font color='{{c:BattleBoosterXP}}'>{{BattleBoosterXP}}</font>",
                "\n<font color='{{c:BattleEventXP}}'>{{BattleEventXP}}</font>",
                "\n<font color='{{c:BattleOriginalXP}}'>{{BattleOriginalXP}}</font>",
                "\n<{{um:height}}='19'><font color='{{c:BattleKills}}'>{{BattleKills}}</font>",
                "\n<font color='{{c:BattleRatioDamageWN8}}'>{{BattleDamageDealt}}</font>{{up:BattleTeamDamage}}",
                "\n<font color='{{c:BattleDamageRatio}}'>{{BattleDamageRatio:1f}}</font>",
                "\n<font color='{{c:BattleAssisted}}'>{{BattleAssisted}}</font>",

                "\n</textformat><textformat leading='-160'>\n<{{um:height}}='5'>\n</textformat>",
                "<img src='img://../configs/BBMods/Kapany3uk/img/clock.png' vspace='-3'>{{BattleDurationReal:t}}</font>",
                "\n<{{um:height}}='8'>\n<font color='{{c:BattleWN8}}'>{{BattleWN8}}</font>",
                "\n<font color='{{c:BattleWN8K}}'>{{BattleWN8K}}</font>",
                "\n<font color='{{c:BattleEFF}}'>{{BattleEFF}}</font>",
                "\n<{{um:height}}='5'>\n<font color='{{c:BattleExpenses}}'>{{BattleRepairCost}}</font>",
                "\n<font color='{{c:BattleExpenses}}'>{{BattleLoadCost}}</font></textformat>",
                "\n<{{um:height}}='13'><font color='{{c:BattleExpenses}}'>{{BattleEquipCost}}</font>",
                "\n<font color='{{c:BattleExpenses}}'>{{BattleContribution}}</font>",
                "\n<font color='{{c:BattleExpenses}}'>{{BattleExpensesG}}</font>",
                "\n<{{um:height}}='5'>\n<font color='{{c:BattleFreeXP}}'>{{BattleFreeXP}}</font>",
                "\n<font color='{{c:BattleBoosterFreeXP}}'>{{BattleBoosterFreeXP}}</font>",
                "\n<font color='{{c:BattleEventFreeXP}}'>{{BattleEventFreeXP}}</font>",
                "\n<font color='{{c:BattleExpenses}}'>{{BattleXPPenalty}}</font>",
                "\n<{{um:height}}='5'>\n<font color='{{c:BattleCapturePoints}}'>{{BattleCapturePoints}}</font>",
                "\n<font color='{{c:BattleDroppedCapturePoints}}'>{{BattleDroppedCapturePoints}}</font>",
                "\n<{{um:height}}='13'><font color='{{c:BattleSpotted}}'>{{BattleSpotted}}</font>",
                "\n<font color='{{c:BattleHitsRatio}}'>{{BattleHitsRatio:1f}}%</font>",
                "</p><{{um:height}}='9'>\n</font></textformat><textformat tabstops='[151]'>",

                "<a href='event{{up:Overall_isReset}}'>{{um:btn_overall}}</a>",
                "<a href='event{{up:Current_isReset}}'>{{um:btn_current}}</a>",
                "<a href='event{{up:Clanwar_isReset}}'>{{um:btn_clanwar}}</a>",
                "<a href='event{{up:Teams_isReset}}'>{{um:btn_teams}}</a>\t",
                "<a href='event:Page:Summary'>{{um:btn_battle_}}</a>",
                "<a href='event:Page:Helpme'>{{um:btn_help}}</a></textformat>"
            ],
            "icon": "",
            "bgIcon": "img://../configs/BBMods/Kapany3uk/img/bg/{{BattleResult}}.png"
        },

        // Раздел "Все бои"
