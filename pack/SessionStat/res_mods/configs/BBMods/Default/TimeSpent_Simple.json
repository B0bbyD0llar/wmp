{
	${"Default/Translation.json"},
	${"Default/StatisticbyTank.json"},
	${"Default/Colors.json"},

	"SkinDescription": {
		"Author": "ShuraBB",
		"Name": "TimeSpent Simple",
		"Version": "3.2",
		"Description": "txt:DescSimple"
	},

	"ShowStatIn": 2,
	"ScrollStep": 15,
	"PopUpResultLife": 12000.0,
	"PopUpResultHiding": 1000.0,
	"OriginalResults": false,
	"InBattleResult": true,

	"StatisticSessions": {
		"Current": {
			"Reset": {"ResetAfter": 0, "ResetTime": 4},
			"BattleTypes": [1,3,7,13,20,21,22,24],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
		}
	},

	"ResultName": {"Win": "txt:Win", "Defeat": "txt:Defeat", "Draw": "txt:Draw"},

	"ResultsWindowPattern": [
		"{{DefaultName}}\t",
		"<img src='img://gui/maps/icons/statistic/wins24.png' width='24' height='24' vspace='-5'><font color='{{c:Winrate}}'>{{Winrate:2f}}%</font> ( <font color='{{c:WinsNextWinrate}}'>{{WinsNextWinrate}}</font>|<font color='{{c:Current_NumWinsReserve}}'>{{Current_NumWinsReserve:0f}}</font> )\t",
		"{{up:BattleMarksOnGunIcon}}( <font color='{{c:BattleAssistedDamageRating}}'>{{BattleAssistedDamageRating}}</font> )"
	],

	"Pages": {
		"First": "Current",
		"NoBattle": {
			"template": [
				"<p align='center'><font color='#E0E0E0' size='16'>{{UserName}}\ntxt:NoBattle</font></p>\n\n",
				"<font size='12' color='#545454'>",
				"txt:Mod: <a href='event:{{ModURL}}'><font color='#545454'>{{ModVersion}}</font></a>\n",
				"txt:Skin: '{{SkinName}}' ver:{{SkinVersion}}\n",
				"txt:Autor: {{SkinAuthor}}\n\n",
				"<p align='justify'>{{SkinDescription}}</p></font>"
			],
			"icon": "",
			"bgIcon": ""
		},
		"Current": {
			"template": [
				"<textformat leading='1' tabstops='78,155'>",
				"WGR: <font color='{{c:WGR}}'><b>{{WGR}}</b></font>\tWN8: <font color='{{c:Current_WN8}}'><b>{{Current_WN8}}</b></font>\tEFF: <font color='{{c:Current_EFF}}'><b>{{Current_EFF}}</b></font>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>\n",
				"<textformat leading='0' tabstops='110,164'>",
				"txt:BatWin:\t<b>{{Current_BattleCounter}}/{{Current_WinCounter}}\t( <font color='{{c:Current_WinCounter:r}}'>{{Current_WinCounter:r}}%</font> )</b>\n",
				"txt:Dest:\t<b>{{Current_Kills}}\t( <font color='{{c:Current_KillsAvg}}'>{{Current_KillsAvg:2f}}</font> )</b>\n",
				"txt:Exp:\t<b>{{Current_OriginalXP}}\t( <font color='{{c:Current_OriginalXPAvg}}'>{{Current_OriginalXPAvg}}</font> )</b>\n",
				"txt:Damage:\t<b>{{Current_DamageDealt}}\t( <font color='{{c:Current_DamageDealtAvg}}'>{{Current_DamageDealtAvg}}</font> )</b>\n",
				"txt:Profit:\t<b>{{Current_CreditDebit}}\t( <font color='{{c:Current_CreditDebitAvg}}'>{{Current_CreditDebitAvg}}</font> )</b>\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1'>\n",
				"<p align='center'>\n{{um:ButtonMaps}}   {{um:ButtonTanks}}   {{um:ButtonReset}}</p>"
			],
			"icon": "img://gui/maps/icons/library/ClanBattleResultIcon-1.png",
			"bgIcon": "img://../res_mods/configs/BBMods/Default/bg.png"
		},
		"Maps": {
			"template": [
				"<textformat leading='0' tabstops='30,137,160,200'>",
				"\ttxt:Map\t<img src='img://gui/maps/icons/statistic/battles24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/wins24.png' width='24' height='24' vspace='-7'>\tУр\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1' vspace='0'>\n",
				"<textformat tabstops='145,165,197'>{{Current_MapsStat}}",
				"<p align='center'>\n{{um:ButtonCurrent}}   {{um:ButtonTanks}}</p>"
			],
			"bgIcon": "img://../res_mods/configs/BBMods/Default/bg.png"
		},
		"Tanks": {
			"template": [
				"<textformat leading='0' tabstops='25,88,113,153,192'>",
				"\t<img src='img://gui/maps/icons/buttons/Tank-ico.png' width='28' height='12' vspace='1'>",
				"\t<img src='img://gui/maps/icons/statistic/battles24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/wins24.png' width='24' height='24' vspace='-7'>",
				"\t<img src='img://gui/maps/icons/statistic/avgDamage24.png' width='24' height='24' vspace='-7'>\tWN8\n",
				"<img src='img://gui/maps/icons/map/entry/01_shot_sector_line.png' width='250' height='1' vspace='0'>\n",
				"<textformat tabstops='21,97,115,152,190'>{{Current_TanksStat}}",
				"<p align='center'>\n{{um:ButtonCurrent}}   {{um:ButtonMaps}}</p>"
			],
			"bgIcon": "img://../res_mods/configs/BBMods/Default/bg.png"
		}
	},

	"Current_MapsStat": {
		"SortKey": "BattleCounter#des",
		"template": ["<font color='#A1A1A1'>{{Name}}</font>\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>\t<font color='{{c:BattleLevelAvg}}'>{{BattleLevelAvg:1f}}</font>\n"]
	},

	"Current_TanksStat": {
		"SortKey": "BattleCounter#des",
		"template": [
			"{{up:MarkOfMastery}}\t<font color='#A1A1A1'>{{TankNameShort}}</font>",
			"\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>",
			"\t<font color='{{c:DamageDealtAvg}}'>{{DamageDealtAvg}}</font>\t<font color='{{c:XWN8}}'>{{WN8}}</font>\n"
		]
	},

	"InBattleResultPattern": ["<p align='center'><font color='{{c:BattleResult}}' size='18'>{{BattleResultName}}: {{BattleMapName}} - {{BattleTankName}}</font></p>"],

	"ResultPattern": {
		"template": ["{{um:Results}}"]
	},

	"UserMacros": {
		"um:Results": [
			"<p align='center'><font color='{{c:BattleResult}}' size='16'><b>{{BattleTankName}}\n{{BattleMapName}}</b></font>\n<b>{{up:BattleMastery}}\t{{up:BattleDamageRating}}</p>",
			"<b>txt:Damage: <font color='{{c:BattleDamageDealt}}'>{{BattleDamageDealt}}</font>   txt:Dest: <font color='{{c:BattleKills}}'>{{BattleKills}}</font>\n",
			"txt:Credits: <font color='{{c:BattleCreditDebit}}'>{{BattleCreditDebit}}</font> ( <font color='{{c:BattleCredits}}'>{{BattleCredits}}</font> <font color='#FE7903'>- {{BattleExpenses}}</font> )\n",
			"txt:Exp: <font color='{{c:BattleXP}}'>{{BattleXP}}</font> / <font color='{{c:BattleTmenXP}}'>{{BattleTmenXP}}</font></b> / <font color='{{c:BattleFreeXP}}'>{{BattleFreeXP}}</font>\n",
			"<p align='center'>\n{{um:ButtonResult}}</p>"
		],
		"um:ButtonCurrent": ["<a href='event:Page:Current'><b>txt:bStat</b></a>"],
		"um:ButtonMaps": ["<a href='event:Page:Maps'><b>txt:bMaps</b></a>"],
		"um:ButtonTanks": ["<a href='event:Page:Tanks'><b>txt:bTanks</b></a>"],
		"um:ButtonReset": ["<a href='event:Reset'><b>[R]</b></a>"],
		"um:ButtonResult": ["<a href='event:showBattleResults'>txt:bDetailed</a>"]
	},

	"UserPattern": {
		"up:BattleMastery": ["<img src='img://gui/maps/icons/library/proficiency/class_icons_{{BattleMastery}}.png' width='28' height='28' vspace='-10'>", ""],
		"up:MarkOfMastery": ["<img src='img://gui/maps/icons/library/proficiency/class_icons_{{MarkOfMastery}}.png' width='20' height='20' vspace='-6'>", ""],
		"up:BattleDamageRating": ["<img src='img://gui/maps/icons/marksOnGun/95x85/{{BattleTankNation}}_1_mark.png' width='15' height='15' vspace='-8'><font color='{{c:BattleDamageRating}}'>{{BattleDamageRating:2f}}%</font> (<font color='{{c:BattleDiffDamageRating}}'>{{BattleDiffDamageRating:2f}})</font>", ""],
		"up:BattleMarksOnGunIcon": ["<img src='img://gui/maps/icons/marksOnGun/95x85/{{BattleMarksOnGunIcon}}' width='15' height='15' vspace='-8'><font color='{{c:BattleDamageRating}}'>{{BattleDamageRating:2f}}%</font><img src='img://../res_mods/configs/BBMods/Default/{{c:BattleDiffDamageRating}}' vspace='-3'>", ""]
	}
}
